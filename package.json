{
  "name": "@angelsit/react-native-wheel-picker",
  "version": "1.0.0",
  "description": "Wheel picker for React Native",
  "main": "index.ts",
  "scripts": {
    "test": "npm run lint",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "format": "prettier --parser typescript --write",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ],
    "*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/angelsit-mobile/react-native-wheel-picker.git"
  },
  "keywords": [
    "WheelPicker",
    "React",
    "Native",
    "react-native",
    "wheel-picker",
    "DatePicker",
    "date-picker",
    "angelsit",
    "mobile",
    "rn",
    "rn-wheel-picker",
    "wheel-date-picker",
    "date"
  ],
  "author": "Viktor Karpov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angelsit-mobile/react-native-wheel-picker/issues"
  },
  "homepage": "https://github.com/angelsit-mobile/react-native-wheel-picker#readme",
  "dependencies": {
    "moment": "^2.24.0"
  },
  "peerDependencies": {
    "moment": ">=2.0.0",
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/react": "^16.9.3",
    "@types/react-native": "^0.60.16",
    "husky": "^3.0.5",
    "lint-staged": "^9.4.0",
    "prettier": "^1.18.2",
    "tslib": "^1.10.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.6.3"
  }
}
